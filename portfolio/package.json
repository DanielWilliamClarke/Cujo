{
  "name": "portfolio",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "dev": "source .env && pnpm clear-cache && next dev",
    "build": "next build",
    "start": "NODE_OPTIONS='-r newrelic' next start",
    "lint": "next lint",
    "format": "npx prettier . --write",
    "cf-export": "contentful space export --config contentful_cache/export-config.json --management-token $CONTENTFUL_MANAGEMENT_TOKEN --space-id $CONTENTFUL_SPACE_ID",
    "cf-generate-types": "cf-content-types-generator contentful_cache/export.json --out src/types/contentful && ./tools/fix_contentful_types.sh",
    "clear-cache": "rm -rf ./.next"
  },
  "dependencies": {
    "@contentful/rich-text-plain-text-renderer": "^16.2.10",
    "@contentful/rich-text-react-renderer": "^15.22.11",
    "@contentful/rich-text-types": "^16.8.5",
    "contentful": "^11.0.3",
    "@types/p5": "^1.7.6",
    "dayjs": "^1.11.13",
    "framer-motion": "^12.4.10",
    "next": "14.2.24",
    "p5": "^1.11.3",
    "react": "^18",
    "react-dom": "^18",
    "sharp": "^0.33.5",
    "simplex-noise": "^4.0.3",
    "weighted": "^1.0.0"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "cf-content-types-generator": "^2.15.5",

    "contentful-cli": "^3.3.11",
    "eslint": "^8",
    "eslint-config-next": "14.2.24",
    "postcss": "^8",
    "tailwindcss": "^3.4.1",
    "typescript": "^5"
  },
  "packageManager": "pnpm@9.15.3+sha512.1f79bc245a66eb0b07c5d4d83131240774642caaa86ef7d0434ab47c0d16f66b04e21e0c086eb61e62c77efc4d7f7ec071afad3796af64892fae66509173893a"
}
