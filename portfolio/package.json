{
  "name": "dwc-portfolio",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "preinstall": "npm install --package-lock-only --ignore-scripts && npx npm-force-resolutions",
    "start": "craco start",
    "build": "craco build && npm run compress",
    "build:ci": "CI=true npm run build",
    "build:prod": "NODE_ENV=production GENERATE_SOURCEMAP=false npm run build:ci",
    "compress": "gzipper compress --verbose ./build",
    "watch": "npm-watch",
    "test": "CI=true craco test",
    "eject": "react-scripts eject",
    "sasslint": "sass-lint -v",
    "sasslint:fix": "sass-lint-auto-fix",
    "clean": "rm -rf node_modules && rm -rf package-lock.json",
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "analyze-deps": "cross-env REACT_APP_INTERACTIVE_ANALYZE=1 npm run build",
    "lint": "eslint src --ext .js,.ts,.tsx --max-warnings=0",
    "fix": "eslint src --ext .js,.ts,.tsx --fix"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "watch": {
    "build": {
      "patterns": [
        "src"
      ],
      "extensions": "js,jsx,ts,tsx,css,scss,json,jpg,png,gif,svg"
    }
  },
  "dependencies": {
    "@contentful/rich-text-plain-text-renderer": "^15.11.1",
    "@contentful/rich-text-react-renderer": "^15.11.1",
    "@lottiefiles/react-lottie-player": "^3.4.7",
    "@react-pdf/font": "2.2.0",
    "@react-pdf/renderer": "2.1.0",
    "axios": "^0.21.1",
    "bootstrap": "^5.1.0",
    "color-interpolate": "^1.0.5",
    "dat.gui": "^0.7.7",
    "detect-browser": "^5.3.0",
    "graphql": "^16.5.0",
    "inversify": "^6.0.1",
    "inversify-react": "^1.0.2",
    "logrocket": "^2.2.1",
    "logrocket-react": "^5.0.1",
    "moment": "^2.29.1",
    "p5": "^1.3.1",
    "pdfjs-dist": "^2.14.305",
    "react": "^17.0.2",
    "react-awesome-reveal": "^3.8.1",
    "react-bootstrap": "^2.0.0-beta.5",
    "react-circular-progressbar": "^2.0.4",
    "react-dom": "^17.0.2",
    "react-helmet": "^6.1.0",
    "react-router-dom": "^5.2.0",
    "react-scrollspy": "^3.4.3",
    "react-share": "^4.4.0",
    "react-syntax-highlighter": "^15.5.0",
    "react-vertical-timeline-component": "^3.3.3",
    "react-visibility-sensor": "^5.1.1",
    "reading-time": "^1.5.0",
    "reflect-metadata": "^0.1.13",
    "simplex-noise": "^2.4.0",
    "styled-components": "^5.3.5",
    "styled-icons": "^10.45.0",
    "urql": "^2.2.3",
    "util": "^0.12.4"
  },
  "devDependencies": {
    "@craco/craco": "^6.4.5",
    "@types/dat.gui": "^0.7.7",
    "@types/logrocket-react": "^3.0.0",
    "@types/node": "^15.12.4",
    "@types/p5": "^1.3.0",
    "@types/pdfjs-dist": "^2.10.378",
    "@types/react": "^17.0.11",
    "@types/react-dom": "^17.0.8",
    "@types/react-helmet": "^6.1.5",
    "@types/react-router-dom": "^5.1.7",
    "@types/react-scrollspy": "^3.3.4",
    "@types/react-syntax-highlighter": "^13.5.2",
    "@types/react-vertical-timeline-component": "^3.0.1",
    "craco-esbuild": "^0.5.0",
    "craco-graphql-loader": "^1.0.1",
    "cross-env": "^7.0.3",
    "gzipper": "^7.1.0",
    "npm-watch": "^0.10.0",
    "react-scripts": "^4.0.3",
    "sass": "^1.35.1",
    "sass-lint-auto-fix": "^0.21.2",
    "source-map-explorer": "^2.5.2",
    "typescript": "^4.4.4",
    "webpack-bundle-analyzer": "^4.5.0",
    "worker-loader": "^3.0.8"
  },
  "resolutions": {
    "@react-pdf/font": "2.2.0",
    "coa": "2.0.2",
    "@types/react": "17.0.11",
    "@types/react-dom": "17.0.8"
  }
}
