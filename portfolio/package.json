{
  "name": "dwc-portfolio",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "preinstall": "npm install --package-lock-only --ignore-scripts && npx npm-force-resolutions",
    "start": "craco start",
    "build": "craco build",
    "build:ci": "CI=true craco build",
    "build:prod": "CI=true GENERATE_SOURCEMAP=false npm run build",
    "watch": "npm-watch",
    "test": "CI=true craco test",
    "eject": "react-scripts eject",
    "sasslint": "sass-lint -v",
    "sasslint:fix": "sass-lint-auto-fix",
    "clean": "rm -rf node_modules && rm -rf package-lock.json"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "watch": {
    "build": {
      "patterns": [
        "src"
      ],
      "extensions": "js,jsx,ts,tsx,css,scss,json,jpg,png,gif,svg"
    }
  },
