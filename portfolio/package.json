{
  "name": "dwc-portfolio",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "build:prod": "GENERATE_SOURCEMAP=false npm run build",
    "watch": "npm-watch",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "sasslint": "sass-lint -v",
    "sasslint:fix": "sass-lint-auto-fix"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "watch": {
    "build": {
      "patterns": [
        "src"
      ],
      "extensions": "js,jsx,ts,tsx,css,scss,json,jpg,png,gif,svg"
    }
  },
  "dependencies": {
    "axios": "^0.21.1",
    "bootstrap": "^5.1.0",
    "inversify": "^5.1.1",
    "inversify-react": "^1.0.0",
    "moment": "^2.29.1",
    "p5": "^1.3.1",
    "react": "^17.0.2",
    "react-bootstrap": "^2.0.0-beta.5",
    "react-dom": "^17.0.2",
    "react-highlight": "^0.14.0",
    "react-icons": "^4.2.0",
    "react-markdown": "^6.0.2",
    "react-reveal": "^1.2.2",
    "react-router-dom": "^5.2.0",
    "react-scrollspy": "^3.4.3",
    "react-share": "^4.4.0",
    "react-vertical-timeline-component": "^3.3.3",
    "reflect-metadata": "^0.1.13",
    "remark-breaks": "^2.0.2",
    "simplex-noise": "^2.4.0"
  },
  "devDependencies": {
    "@types/node": "^15.12.4",
    "@types/p5": "^1.3.0",
    "@types/react": "^17.0.11",
    "@types/react-dom": "^17.0.8",
    "@types/react-router-dom": "^5.1.7",
    "@types/react-scrollspy": "^3.3.4",
    "@types/react-vertical-timeline-component": "^3.0.1",
    "html-webpack-plugin": "^5.3.1",
    "npm-watch": "^0.10.0",
    "react-scripts": "^4.0.3",
    "sass": "^1.35.1",
    "sass-lint-auto-fix": "^0.21.2",
    "typescript": "^4.3.4"
  }
}
