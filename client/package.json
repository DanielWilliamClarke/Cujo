{
  "name": "modern-cv-client",
  "version": "0.0.1",
  "description": "A modern CV app client implemented using React, GraphQL",
  "main": "index.js",
  "scripts": {
    "setup": "npm install webpack webpack-cli karma-cli nodemon -g && npm install",
    "build": "webpack --mode development",
    "start:dev": "webpack-dev-server --open --mode development",
    "start:watch": "webpack-dev-server --mode development --host 0.0.0.0 --hot --inline",
    "start:docker": "npm run build && nodemon ./index.js",
    "test": "karma start",
    "coverage": "karma start --no-auto-watch --coverage=true || echo done"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DanielWilliamClarke/modern-cv-app.git"
  },
  "keywords": [],
  "author": "Daniel Clarke",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/DanielWilliamClarke/modern-cv-app/issues"
  },
  "homepage": "https://github.com/DanielWilliamClarke/modern-cv-app#readme",
  "dependencies": {
    "apollo-fetch": "^0.7.0",
    "graphql": "^0.13.2",
    "graphql-tag": "^2.9.2",
    "prop-types": "^15.6.1",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-markdown": "^3.3.3"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.4",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "istanbul-instrumenter-loader": "^3.0.1",
    "jasmine-core": "^3.1.0",
    "karma": "^2.0.3",
    "karma-chrome-launcher": "^2.2.0",
    "karma-coverage": "^1.1.2",
    "karma-jasmine": "^1.1.2",
    "karma-jasmine-html-reporter": "^1.1.0",
    "karma-mocha-reporter": "^2.2.5",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^3.0.0",
    "nodemon": "^1.17.5",
    "webpack": "^4.12.0",
    "webpack-cli": "^3.0.7",
    "webpack-dev-server": "^3.1.4"
  }
}
