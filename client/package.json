{
  "name": "client",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "npm run genenv && react-scripts start",
    "build": "react-scripts build",
    "watch": "npm-watch",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "genenv": "chmod +x ./env.sh && ./env.sh && cp env-config.js ./public/"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "watch": {
    "build": {
      "patterns": [
        "src"
      ],
      "extensions": "js,jsx,tsx,css,scss,json"
    }
  },
  "dependencies": {
    "@types/jest": "^26.0.20",
    "@types/node": "^14.14.25",
    "@types/react": "^17.0.1",
    "@types/react-dom": "^17.0.0",
    "axios": "^0.21.1",
    "bootstrap": "^4.6.0",
    "moment": "^2.29.1",
    "react": "^17.0.1",
    "react-bootstrap": "^1.4.3",
    "react-dom": "^17.0.1",
    "react-highlight": "^0.13.0",
    "react-markdown": "^5.0.3",
    "react-reveal": "^1.2.2",
    "react-router-dom": "^5.2.0",
    "react-router-hash-link": "^2.4.0",
    "react-share": "^4.3.1",
    "react-sticky": "^6.0.3",
    "remark-breaks": "^2.0.1",
    "typescript": "^4.1.4",
    "wpapi": "^1.2.1"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.11.9",
    "@testing-library/react": "^11.2.5",
    "@testing-library/user-event": "^12.6.3",
    "@types/react-router-dom": "^5.1.7",
    "@types/react-router-hash-link": "^1.2.1",
    "@types/react-sticky": "^6.0.3",
    "@types/wpapi": "^1.1.0",
    "html-webpack-plugin": "^5.0.0",
    "npm-watch": "^0.7.0",
    "react-scripts": "^4.0.2",
    "sass": "^1.32.7"
  }
}
